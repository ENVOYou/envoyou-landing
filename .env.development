# Environment Configuration for EnvoyOU

# Application
VITE_APP_NAME=EnvoyOU
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Professional CV Enhancement API Service

# Environment
VITE_NODE_ENV=development
VITE_APP_ENV=development

# API Configuration
VITE_API_BASE_URL=http://localhost:3001/api
VITE_API_VERSION=v1
VITE_API_TIMEOUT=30000

# Authentication
VITE_JWT_SECRET=your-super-secret-jwt-key-change-in-production
VITE_JWT_EXPIRES_IN=7d
VITE_SESSION_TIMEOUT=3600000

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false
VITE_ENABLE_PWA=true
VITE_ENABLE_SERVICE_WORKER=true

# External Services
VITE_GOOGLE_ANALYTICS_ID=
VITE_SENTRY_DSN=
VITE_STRIPE_PUBLISHABLE_KEY=

# Development Tools
VITE_ENABLE_DEVTOOLS=true
VITE_ENABLE_API_MOCKING=true
VITE_SHOW_API_TESTER=true

# Contact & Support
VITE_SUPPORT_EMAIL=support@envoyou.com
VITE_CONTACT_EMAIL=contact@envoyou.com
VITE_LEGAL_EMAIL=legal@envoyou.com

# Social Media
VITE_GITHUB_URL=https://github.com/hk-dev13
VITE_LINKEDIN_URL=https://www.linkedin.com/in/husni-kusuma/
VITE_TWITTER_URL=https://x.com/EnvoyouAPI

# Rate Limiting
VITE_API_RATE_LIMIT_REQUESTS=100
VITE_API_RATE_LIMIT_WINDOW=60000
